{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "template_flutter_claude",
      "request": "launch",
      "type": "dart",
      "program": "lib/main.dart",
      "cwd": "${workspaceFolder}",
      "toolArgs": [
        "--dart-define-from-file",
        "${workspaceFolder}/env/.dev.env"
      ]
    },
    {
      "name": "template_flutter_claude (profile mode)",
      "request": "launch",
      "type": "dart",
      "program": "lib/main.dart",
      "cwd": "${workspaceFolder}",
      "flutterMode": "profile"
    },
    {
      "name": "template_flutter_claude (release mode)",
      "request": "launch",
      "type": "dart",
      "program": "lib/main.dart",
      "cwd": "${workspaceFolder}",
      "flutterMode": "release"
    }
  ]
}
