{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "take_short",
      "request": "launch",
      "type": "dart",
      "cwd": "${workspaceFolder}/app",
      "toolArgs": [
        "--dart-define-from-file",
        "${workspaceFolder}/app/env/.dev.env"
      ]
    },
    {
      "name": "take_short (profile mode)",
      "request": "launch",
      "type": "dart",
      "cwd": "${workspaceFolder}/app",
      "flutterMode": "profile"
    },
    {
      "name": "take_short (release mode)",
      "request": "launch",
      "type": "dart",
      "cwd": "${workspaceFolder}/app",
      "flutterMode": "release"
    }
  ]
}
